      "target": "api::project.project",
      "inversedBy": "services"
    },
    "descripton": {
      "pluginOptions": {},
      "type": "blocks",
      "required": true
    },
    "technologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology",
      "mappedBy": "service"
    },
    "details": {
      "type": "component",
      "repeatable": true,
      "component": "services.service-component"
